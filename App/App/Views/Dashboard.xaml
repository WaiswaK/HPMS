             x:Class="App.Views.Dashboard"
             xmlns:pages="clr-namespace:App.Views"
             Title="HPMS">
    
  <MasterDetailPage.Master>
    <pages:DashboardMaster x:Name="MasterPage" />
  </MasterDetailPage.Master>
    <MasterDetailPage.Detail>
        <!--<NavigationPage BarBackgroundColor="White" BarTextColor="White">-->
        <NavigationPage BarTextColor="White">
            <x:Arguments>
                <pages:DashboardDetail/>
            </x:Arguments>
        </NavigationPage>
    </MasterDetailPage.Detail>
</MasterDetailPage>