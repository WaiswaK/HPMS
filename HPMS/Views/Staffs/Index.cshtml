@model IEnumerable<HPMS.Models.Staff>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Demographic.Given_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Job_Description)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Demographic.Given_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Job_Description)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.SID }) |
            @Html.ActionLink("Details", "Details", new { id=item.SID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.SID })
        </td>
    </tr>
}

</table>
